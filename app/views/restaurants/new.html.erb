<div class="container">
  <h1> Adding a new restaurant</h1>
  <%= simple_form_for(@restaurant) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :contact_number %>
    <%= f.input :photos, as: :file, input_html: { multiple: true} %>
    <%= f.submit %>
  <% end %>
</div>
